<view class="yr-ui-parse__wrap">
  <block wx:for="{{nodesData}}" wx:key="index">
    <!-- 判断是否为标签节点 -->
    <block wx:if="{{item.node == 'element'}}">
      <!-- button类型 -->
      <block wx:if="{{item.tag == 'button'}}">
        <button type="default" size="mini">
          <!-- 如果还有子节点，递归遍历自身 -->
          <block wx:for="{{item.nodes}}" wx:for-item="child" wx:for-index="idx" wx:key="idx">
            <yr-ui-parse nodes="{{child}}" nodeKey="{{pageNodeKey}}" imgPreview="{{imgPreview}}" thBgcolor="{{thBgcolor}}" bind:atap="onAtap" bind:preview="onPreview"></yr-ui-parse>
          </block>
        </button>
      </block>

      <!-- code类型 -->
      <block wx:elif="{{item && item.tag == 'code'}}">
        <view class="mb10 {{item.classStr}}" style="{{item.styleStr}}">
          <yr-ui-code codeText="{{item.content}}" language="{{item.attr && item.attr.lang}}"></yr-ui-code>
        </view>
      </block>

      <!-- ol类型 -->
      <block wx:elif="{{item.tag == 'ol'}}">
        <view class="fuiParse-ol {{item.classStr}}" style="{{item.styleStr}}">
          <block wx:for="{{item.nodes}}" wx:for-item="child" wx:for-index="idx" wx:key="idx">
            <view class="fuiParse-ol-inner">
              <view class="fuiParse-ol-number">{{idx + 1}}. </view>
              <view class="flex-full overflow-hide">
                <yr-ui-parse nodes="{{child}}" nodeKey="{{pageNodeKey}}" imgPreview="{{imgPreview}}" thBgcolor="{{thBgcolor}}" bind:atap="onAtap" bind:preview="onPreview"></yr-ui-parse>
              </view>
            </view>
          </block>
        </view>
      </block>

      <!-- ul类型 -->
      <block wx:elif="{{item.tag == 'ul'}}">
        <view class="fuiParse-ul {{item.classStr}}" style="{{item.style && item.style.Str}}">
          <block wx:for="{{item.nodes}}" wx:for-item="child" wx:for-index="idx" wx:key="idx">
            <view class="fuiParse-ul-inner">
              <view class="fuiParse-li-circle"></view>
              <view class="flex-full overflow-hide">
                <yr-ui-parse nodes="{{child}}" nodeKey="{{pageNodeKey}}" imgPreview="{{imgPreview}}" thBgcolor="{{thBgcolor}}" bind:atap="onAtap" bind:preview="onPreview"></yr-ui-parse>
              </view>
            </view>
          </block>
        </view>
      </block>

      <!-- li类型 -->
      <block wx:elif="{{item.tag == 'li'}}">
        <view class="fuiParse-li {{item.classStr}}" style="{{item.styleStr}}">
          <block wx:for="{{item.nodes}}" wx:for-item="child" wx:for-index="idx" wx:key="idx">
            <yr-ui-parse nodes="{{child}}" nodeKey="{{pageNodeKey}}" imgPreview="{{imgPreview}}" thBgcolor="{{thBgcolor}}" bind:atap="onAtap" bind:preview="onPreview"></yr-ui-parse>
          </block>
        </view>
      </block>

      <!-- video类型 -->
      <block wx:elif="{{item.tag == 'video'}}">
        <!--增加video标签支持，并循环添加-->
        <view class="{{item.classStr}} {{'fuiParse-'+item.tag}}" style="{{item.styleStr}}">
          <video class="{{item.classStr}} {{'fuiParse-'+item.tag+'-video'}}" src="{{item.attr && item.attr.src}}"></video>
        </view>
      </block>

      <!-- img类型 -->
      <block wx:elif="{{item.tag == 'img'}}">
        <view class="fuiParse-img-inner" wx:if="{{item.attr && item.attr.src}}">
          <image class="{{item.classStr}} {{'fuiParse-'+item.tag}} {{item.loaded ? 'fuiParse-img-fadein' : ''}}" data-from="{{item.from}}" data-src="{{item.attr.src}}" data-idx="{{item.imgIndex}}" lazy-load="{{true}}" src="{{item.loaded ? item.attr.src : ''}}" bindtap="fuiParseImgTap" mode="{{mode}}" style="width:{{item.attr.width || width}}px;height:{{item.attr.height || height}}px;{{item.styleStr}}" />
          <image class="fuiParse-img__loading {{item.loaded?'fuiParse-img__hidden':''}}" src="data:image/png;base64," />
          <image style="display: none;" mode="{{mode}}" data-from="{{item.from}}" data-index="{{index}}" src="{{item.attr.src}}" bindload="fuiParseImgLoad" />
        </view>
      </block>

      <!-- a类型 -->
      <block wx:elif="{{item.tag == 'a'}}">
        <view bindtap="fuiParseTagATap" class="fuiParse-inline  {{item.classStr}} {{'fuiParse-'+item.tag}}" data-title="{{item.attr && item.attr.title}}" data-src="{{item.attr && item.attr.href}}" style="{{item.styleStr}}">
          <block wx:for="{{item.nodes}}" wx:for-item="child" wx:for-index="idx" wx:key="idx">
            <yr-ui-parse nodes="{{child}}" nodeKey="{{pageNodeKey}}" imgPreview="{{imgPreview}}" thBgcolor="{{thBgcolor}}" bind:atap="onAtap" bind:preview="onPreview"></yr-ui-parse>
          </block>
        </view>
      </block>

      <!-- table类型 -->
      <block wx:elif="{{item.tag == 'table'}}">
        <view class="{{item.classStr}} fuiParse-{{item.tag}}">
          <block wx:for="{{item.nodes}}" wx:for-item="child" wx:for-index="idx" wx:key="idx">
            <yr-ui-parse nodes="{{child}}" nodeKey="{{pageNodeKey}}" imgPreview="{{imgPreview}}" thBgcolor="{{thBgcolor}}" bind:atap="onAtap" bind:preview="onPreview"></yr-ui-parse>
          </block>
        </view>
      </block>

      <!-- tr 类型 -->
      <block wx:elif="{{item.tag == 'tr'}}">
        <view class="{{item.classStr}} fuiParse-{{item.tag}}">
          <block wx:for="{{item.nodes}}" wx:for-item="child" wx:for-index="idx" wx:key="idx">
            <yr-ui-parse class="{{child.classStr}} fuiParse-{{child.tag}} fuiParse-{{child.tag}}-container {{child.tag=='th' && thBgcolor?'fuiParse-th__bg':''}}" style="{{child.styleStr}}" nodes="{{child}}" nodeKey="{{pageNodeKey}}" imgPreview="{{imgPreview}}" thBgcolor="{{thBgcolor}}" bind:atap="onAtap" bind:preview="onPreview"></yr-ui-parse>
          </block>
        </view>
      </block>

      <!-- td 类型 -->
      <block wx:elif="{{item.tag == 'td'}}">
        <view class="{{item.classStr}} fuiParse-{{item.tag}}">
          <block wx:for="{{item.nodes}}" wx:for-item="child" wx:for-index="idx" wx:key="idx">
            <yr-ui-parse class="{{child.classStr}} fuiParse-{{child.tag}} fuiParse-{{child.tag}}-container" style="{{child.styleStr}}" nodeKey="{{pageNodeKey}}" nodes="{{child}}" imgPreview="{{imgPreview}}" thBgcolor="{{thBgcolor}}" bind:atap="onAtap" bind:preview="onPreview"></yr-ui-parse>
          </block>
        </view>
      </block>

      <!-- audio类型 -->
      <block wx:elif="{{item.tag == 'audio'}}">
        <view class="fuiParse-audio">
          <yr-ui-audio class="fuiParse-audio-inner {{item.classStr}}" src="{{item.attr && item.attr.src}}" title="{{item.attr && item.attr.title}}" desc="{{item.attr && item.attr.desc}}" style="{{item.styleStr}}"></yr-ui-audio>
        </view>
      </block>

      <!-- br类型 -->
      <block wx:elif="{{item.tag == 'br'}}">
        <text>\n</text>
      </block>

      <!-- 其它块级标签 -->
      <block wx:elif="{{item.tagType == 'block'}}">
        <view class="{{item.classStr}} fuiParse-{{item.tag}}" style="{{item.styleStr}}">
          <block wx:for="{{item.nodes}}" wx:for-item="child" wx:for-index="idx" wx:key="idx">
            <yr-ui-parse styleStr="{{item.styleStr || ''}}" nodes="{{child}}" nodeKey="{{pageNodeKey}}" imgPreview="{{imgPreview}}" thBgcolor="{{thBgcolor}}" bind:atap="onAtap" bind:preview="onPreview"></yr-ui-parse>
          </block>
        </view>
      </block>

      <!-- 其它内联标签 -->
      <view wx:else class="{{item.classStr}} fuiParse-{{item.tag}} fuiParse-{{item.tagType}}" style="{{item.styleStr}}">
        <block wx:for="{{item.nodes}}" wx:for-item="child" wx:for-index="idx" wx:key="idx">
          <yr-ui-parse nodes="{{child}}" nodeKey="{{pageNodeKey}}" imgPreview="{{imgPreview}}" thBgcolor="{{thBgcolor}}" bind:atap="onAtap" bind:preview="onPreview"></yr-ui-parse>
        </block>
      </view>
    </block>

    <!-- 判断是否为文本节点 -->
    <block wx:elif="{{item.node == 'text'}}">
      <view class="fuiEmojiView fuiParse-inline" style="{{item.styleStr}}">
        <block wx:for="{{item.textArray}}" wx:for-item="textItem" wx:for-index="idx" wx:key="idx">
          <block class="{{textItem.text == '\\n' ? 'fuiParse-hide':''}}" wx:if="{{textItem.node == 'text'}}">
            <text style="{{styleStr}}" user-select="{{true}}" class="fuiParse-text">{{textItem.text}}</text>
          </block>
          <block wx:elif="{{textItem.node == 'element'}}">
            <image class="fuiEmoji" src="{{textItem.baseSrc+textItem.text}}" />
          </block>
        </block>
      </view>
    </block>
  </block>
</view>