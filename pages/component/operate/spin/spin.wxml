<view class="yr-ui-wrap">
		<view class="yr-ui-page__hd">
			<view class="yr-ui-page__title yr-ui-align__center" bindtap="vip">Spin <image
					src="/static/images/index/light/icon_member_3x.png"></image>
			</view>
			<view class="yr-ui-page__desc">Spin 旋转动画。</view>
		</view>
		<view class="yr-ui-page__bd yr-ui-page__spacing">
			<view class="yr-ui-section__title">基础使用</view>
			<yr-ui-grid>
				<yr-ui-grid-item highlight="{{false}}">
					<view class="yr-ui-load__item">
						<yr-ui-spin>旋转</yr-ui-spin>
					</view>
				</yr-ui-grid-item>
				<yr-ui-grid-item highlight="{{false}}">
					<view class="yr-ui-load__item">
						<yr-ui-spin>
							<view class="yr-ui-loading"></view>
						</yr-ui-spin>
					</view>
				</yr-ui-grid-item>
				<yr-ui-grid-item highlight="{{false}}">
					<view class="yr-ui-load__item">
						<yr-ui-spin>
							<view class="yr-ui-loading2"></view>
						</yr-ui-spin>
					</view>
				</yr-ui-grid-item>
				<yr-ui-grid-item highlight="{{false}}">
					<view class="yr-ui-load__item">
						<yr-ui-spin>
							<yr-ui-icon name="loading" color="#FFB703"></yr-ui-icon>
						</yr-ui-spin>
					</view>
				</yr-ui-grid-item>
				<yr-ui-grid-item highlight="{{false}}">
					<view class="yr-ui-load__item">
						<yr-ui-spin>
							<yr-ui-icon name="moments" color="#09BE4F"></yr-ui-icon>
						</yr-ui-spin>
					</view>
				</yr-ui-grid-item>
				<yr-ui-grid-item highlight="{{false}}">
					<view class="yr-ui-load__item">
						<yr-ui-spin>
							<yr-ui-icon name="aftersale" color="#FF2B2B"></yr-ui-icon>
						</yr-ui-spin>
					</view>
				</yr-ui-grid-item>
				<yr-ui-grid-item highlight="{{false}}">
					<view class="yr-ui-load__item">
						<yr-ui-spin>
							<image src="/static/images/component/loading_gray.png" class="yr-ui-icon__img"></image>
						</yr-ui-spin>
					</view>
				</yr-ui-grid-item>
				<yr-ui-grid-item highlight="{{false}}">
					<view class="yr-ui-load__item">
						<yr-ui-spin>
							<image src="data:image/png;base64," class="yr-ui-icon__img"></image>
						</yr-ui-spin>
					</view>
				</yr-ui-grid-item>
				<yr-ui-grid-item highlight="{{false}}">
					<view class="yr-ui-load__item">
						<yr-ui-spin>
							<image src="/static/images/common/img_logo.png" class="yr-ui-icon__img yr-ui-circle"></image>
						</yr-ui-spin>
					</view>
				</yr-ui-grid-item>
			</yr-ui-grid>

		</view>
	</view>