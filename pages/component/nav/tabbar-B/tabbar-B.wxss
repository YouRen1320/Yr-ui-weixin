
page {
  font-weight: normal;
}
.yr-ui-wrap {
  width: 100%;
  padding-bottom: constant(safe-area-inset-bottom);
  padding-bottom: env(safe-area-inset-bottom);
  box-sizing: border-box;
}

.yr-ui-swiper__wrap {
  flex-direction: column;
  height: 100vh;
}

.yr-ui-swiper__item {
  flex-direction: row;
}

.yr-ui-scroll__wrap {
  width: 100%;
  flex-direction: column;
}

.yr-ui-tabbar__h {
  width: 750rpx;
}

.yr-ui-text {
  font-size: 28rpx;
  color: #B2B2B2;
  padding: 32rpx;
  box-sizing: border-box;
}