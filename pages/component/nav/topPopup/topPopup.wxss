page {
  font-weight: normal;
}

.yr-ui-page__bd {
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column;
}

.yr-ui-custom__wrap {
  width: 100%;
  height: 520rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}

/* 案例一 start*/
.yr-ui-popup__container {
  width: 100%;
  position: relative;
  display: flex;
  justify-content: center;
  flex-direction: column;
  padding: 24rpx 32rpx;
  box-sizing: border-box;
}

.yr-ui-title {
  font-size: 30rpx;
  font-weight: bold;
  text-align: center;
}

.yr-ui-sub__title {
  display: block;
  text-align: center;
  font-size: 24rpx;
  color: #7F7F7F;
  transform: scale(.9);
}

.yr-ui-descr {
  font-weight: bold;
  padding-top: 64rpx;
}

.yr-ui-sub__descr {
  font-size: 26rpx;
  color: #B2B2B2;
  padding: 32rpx 0;
}

.yr-ui-btn__box {
  display: flex;
  justify-content: center;
  padding: 32rpx 0;
  box-sizing: border-box;
}

.yr-ui-icon__close {
  position: absolute;
  top: 24rpx;
  left: 24rpx;
}

/* 案例一 end*/
.yr-ui-scroll__wrap {
  padding-top: 30rpx;
  padding-bottom: 24rpx;
  position: relative;
}

.yr-ui-scroll__view {
  width: 100%;
  height: 600rpx;
}

.yr-ui-title__pb {
  padding-bottom: 24rpx;
}