<view class="yr-ui-wrap">
		<view class="yr-ui-page__hd">
			<view class="yr-ui-page__title yr-ui-align__center" bindtap="vip">Circle <image
					src="/static/images/index/light/icon_member_3x.png"></image>
			</view>
			<view class="yr-ui-page__desc">Circle 圆形进度条，用于展现某个任务的当前进度。</view>
		</view>
		<view class="yr-ui-page__bd yr-ui-page__spacing">
			<view class="yr-ui-section__title">基础使用</view>
			<yr-ui-circle percent="{{80}}"></yr-ui-circle>
			<view class="yr-ui-section__title">调整样式/起始弧度/渐变</view>
			<yr-ui-circle percent="{{75}}" color="#FFB703" background="#EEEEEE" foreground="#FFB703" foreground="#FFB703" gradient="#FAE845"
				sAngle="{{-0.5}}" width="{{200}}"	strokeWidth="{{6}}" size="{{16}}"></yr-ui-circle>
			<view class="yr-ui-section__title">调整百分比</view>
			<yr-ui-circle percent="{{percent}}"></yr-ui-circle>
			<view class="yr-ui-btn__box">
				<yr-ui-button type="gray" width="400rpx" height="84rpx" text="percent" bold margin="24rpx"
					bindclick="percentChange"></yr-ui-button>
			</view>
		</view>
	</view>