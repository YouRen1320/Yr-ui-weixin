.yr-ui-section__title {
  width: 300rpx;
  margin-left: 32rpx;
  white-space: nowrap;
}

.yr-ui-outer {
  width: 100%;
  height: 300rpx;
  position: relative;
}

.yr-ui-sk__box {
  width: 100%;
  height: 300rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  background-color: #fff;
}

.yr-ui-sk__wrap {
  background: #16AB60;
  color: #fff;
  width: 200rpx;
  height: 200rpx;
  border-radius: 50%;
  display: flex;
  align-items: center;
  justify-content: center;
  margin: 40rpx auto;
}