page {
  font-weight: normal;
  /* 可使用此变量改变list线条颜色 */
  /* --yr-ui-color-border:#B2B2B2; */
}

.yr-ui-section__title {
  margin-left: 32rpx;
}

.yr-ui-list__item {
  flex: 1;
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.yr-ui-text__explain {
  font-size: 28rpx;
  color: #7F7F7F;
  flex-shrink: 0;
}

.yr-ui-list__icon {
  width: 48rpx;
  height: 48rpx;
  margin-right: 24rpx;
}