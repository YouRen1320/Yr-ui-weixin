<view class="yr-ui-wrap">
  <view class="yr-ui-page__hd">
    <view class="yr-ui-page__title yr-ui-align__center" bindtap="vip">LicensePlate <image src="/static/images/index/light/icon_member_3x.png"></image>
    </view>
    <view class="yr-ui-page__desc">LicensePlate 车牌号键盘，用于车牌号的输入。</view>
  </view>
  <view class="yr-ui-page__bd">
    <text class="yr-ui-title">请输入车牌号</text>
    <view class="yr-ui-si__wrap">
      <text class="yr-ui-si__tip">新能源</text>
      <yr-ui-single-input native="{{false}}" borderColor="#ccc" border="1" borderWidth="1" radius="16" cursorHeight="40" length="{{8}}" width="80" height="80" size="40" padding="32" value="{{value}}" id="fui_si" bindfocus="onFocus">
      </yr-ui-single-input>
    </view>
    <view class="yr-ui-flex__center yr-ui-flex__column">
      <yr-ui-button type="gray" width="400rpx" height="84rpx" text="默认键盘" bold bindclick="btnTap" data-type="1">
      </yr-ui-button>
      <yr-ui-button type="gray" width="400rpx" height="84rpx" text="暗黑模式" bold margin="24rpx 0" bindclick="btnTap" data-type="2"></yr-ui-button>
    </view>
    <yr-ui-license-plate show="{{show}}" theme="{{theme}}" toolbar="{{toolbar}}" bindclick="keyClick" bindbackspace="backspace" bindcomplete="complete"></yr-ui-license-plate>
  </view>
</view>