<view class="yr-ui-wrap">
  <view class="yr-ui-page__hd">
    <view class="yr-ui-page__title yr-ui-align__center" bindtap="vip">Result <image src="/static/images/index/light/icon_member_3x.png"></image>
    </view>
    <view class="yr-ui-page__desc">Result 结果页，在页面中组织图标、文字等内容，向用户反馈操作结果。</view>
  </view>
  <view class="yr-ui-page__bd">
    <yr-ui-button type="gray" width="400rpx" height="84rpx" text="成功 success" bold margin="24rpx 0" bindclick="href" data-type="1">
    </yr-ui-button>
    <yr-ui-button type="gray" width="400rpx" height="84rpx" text="警告 warning" bold bindclick="href" data-type="2">
    </yr-ui-button>
    <yr-ui-button type="gray" width="400rpx" height="84rpx" text="失败 fail" bold margin="24rpx 0" bindclick="href" data-type="3">
    </yr-ui-button>
    <yr-ui-button type="gray" width="400rpx" height="84rpx" text="等待 waiting" bold bindclick="href" data-type="4">
    </yr-ui-button>
    <yr-ui-button type="gray" width="400rpx" height="84rpx" text="自定义图标" bold margin="24rpx 0" bindclick="href" data-type="5">
    </yr-ui-button>
  </view>
</view>