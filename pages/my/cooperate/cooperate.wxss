page {
  font-weight: normal;
  background-color: #fff;
}

.yr-ui-banner__box {
  width: 100%;
  height: 420rpx;
  background: #F1F4FA;
}

.yr-ui-banner {
  width: 100%;
  height: 420rpx;
  display: block;
}

.yr-ui-menu__wrap {
  width: 100%;
  padding: 16rpx 16rpx 32rpx;
  box-sizing: border-box;
  display: flex;
  flex-wrap: wrap;
}

.yr-ui-menu__item {
  width: 33.333333%;
  padding: 16rpx;
  box-sizing: border-box;
}

.yr-ui-item__inner {
  width: 100%;
  height: 208rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column;
  font-size: 28rpx;
  color: #333333;
  background-color: rgba(254, 184, 8, .05);
  border-radius: 24rpx;
}

.yr-ui-bg__1 {
  background-color: rgba(9, 190, 79, .05);
}

.yr-ui-bg__2 {
  background-color: rgba(70, 92, 255, .05);
}

.yr-ui-bg__3 {
  background-color: rgba(104, 49, 255, .05);
}

.yr-ui-bg__4 {
  background-color: rgba(255, 43, 43, .05);
}

.yr-ui-page__bd {
  background-color: #fff;
}

.yr-ui-menu__icon {
  width: 96rpx;
  height: 96rpx;
  display: block;
  margin-bottom: 16rpx;
}

.yr-ui-contacts__box {
  width: 100%;
  padding: 32rpx 32rpx 20rpx;
  box-sizing: border-box;
  display: flex;
  align-items: center;
}

.yr-ui-contacts__item {
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column;
  flex: 1;
}

.yr-ui-icon {
  width: 96rpx;
  height: 96rpx;
}

.yr-ui-title {
  font-size: 28rpx;
  color: #333333;
  padding-top: 16rpx;
}

.yr-ui-desc {
  font-size: 24rpx;
  font-weight: 400;
  color: #7F7F7F;
  line-height: 48rpx;
  width: 100%;
  padding: 0 64rpx;
  box-sizing: border-box;
  text-align: center;
}

.yr-ui-pb {
  padding-bottom: 16rpx;
}

.yr-ui-section__title {
  margin-bottom: 24rpx;
}

.yr-ui-link {
  padding: 0 8rpx;
  font-weight: 500;
  color: #465CFF;
  white-space: nowrap;
}

.yr-ui-divider {
  width: 100%;
  height: 24rpx;
  background-color: #F1F4FA;
}

.yr-ui-ani__box {
  width: 622rpx;
  background: #FFFFFF;
  border-radius: 24rpx;
  padding-bottom: 64rpx;
}

.yr-ui-ani__title {
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 36rpx;
  font-weight: 600;
  color: #465CFF;
  padding: 48rpx 0 30rpx;
}

.yr-ui-ani__title text {
  padding: 0 24rpx;
}

.yr-ui-ani__title image {
  width: 116rpx;
  height: 20rpx;
}

.yr-ui-p {
  color: #333;
  text-align: left;
  margin-bottom: 120rpx;
}